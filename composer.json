{
  "name" 			: "webjen/wordpress-starter-plugin",
  "description" 	: "Dev Helpers WordPress Plugin.",
  "type"            : "project",
  "keywords" 		: ["wordpress", "wp", "plugin"],
  "homepage" 		: "https://jennyryden.com",
  "license" 		: "GPL-2.0+",
  "authors": [
    {
      "name" 	: "jryden",
      "email"   : "jenny@jennyryden.com"
    }
  ],
  "require": {
    "php" : ">=5.4.0"
  },
  "require-dev": {
    "kint-php/kint" : "^1.0",
    "filp/whoops": "~2.0"
  },
  "autoload" : {
    "classmap": [],
    "files": [
      "src/support/exceptions.php",
      "src/sandbox.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "vendor-dir": "assets/vendor"
  },
  "minimum-stability": "dev"
}